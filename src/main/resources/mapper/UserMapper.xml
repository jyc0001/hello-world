<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jyc.rtodemo.mapper.UserMapper">

    <sql id="selectFields">
        id,username,password,register_date,last_login_date
    </sql>

   <select id="selectUserByJdpin" resultType="User">
       select <include refid="selectFields"></include> from user where id = #{id}
   </select>

</mapper>
